<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-test-service-1"  />
    <dubbo:registry protocol="zookeeper" address="192.168.8.18:2181,192.168.8.19:2181,192.168.8.20:2181" client="zkclient" />
    <dubbo:monitor/>
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:service stub="true" timeout="5000" retries="1" connections="50" loadbalance="random" interface="com.self.api.DemoService" ref="demoService" />
    <bean id="demoService" class="com.self.DemoServiceImpl" />

    <bean id="pingService" class="com.self.PingServiceImpl" />
    <dubbo:service stub="true" timeout="5000" retries="1" connections="50" loadbalance="random" interface="com.self.api.PingService" ref="pingService" />
    <dubbo:provider/>

</beans>